{
  "name": "Event",
  "type": "object",
  "properties": {
    "title": {
      "type": "string",
      "description": "Event name"
    },
    "description": {
      "type": "string",
      "description": "Event description"
    },
    "image_url": {
      "type": "string",
      "description": "Event cover image"
    },
    "category": {
      "type": "string",
      "enum": [
        "flagship",
        "hackathon",
        "workshop",
        "mentoring",
        "media",
        "technical",
        "outreach"
      ],
      "description": "Event category"
    },
    "date": {
      "type": "string",
      "description": "Event date"
    },
    "is_featured": {
      "type": "boolean",
      "description": "Whether this is a featured/highlight event"
    }
  },
  "required": [
    "title",
    "description"
  ]
}